<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context=".activity.admin.DetailAdmin">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:gravity="center"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_150sdp"
            android:background="@drawable/linearlayout_blue">
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@drawable/logo2"
                android:layout_gravity="center"/>
        </LinearLayout>
        <LinearLayout
            android:padding="8dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_gravity="center">
            <!--Telp-->
            <LinearLayout
                android:layout_marginTop="25dp"
                android:orientation="horizontal"
                android:paddingStart="25dp"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_75sdp"
                tools:ignore="RtlSymmetry">
                <ImageView
                    android:layout_gravity="center"
                    android:layout_width="@dimen/_30sdp"
                    android:layout_height="@dimen/_30sdp"
                    android:src="@drawable/ic_step"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <TextView
                        android:textSize="@dimen/_12ssp"
                        android:textStyle="bold"
                        android:fontFamily="@font/poppins_medium"
                        android:layout_gravity="center_vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingStart="20dp"
                        android:text="Proses"/>
                    <com.baoyachi.stepview.HorizontalStepView
                        android:layout_width="@dimen/_200sdp"
                        android:layout_height="match_parent"
                        android:id="@+id/horidetailadmin"
                        tools:ignore="RtlSymmetry" />
                </LinearLayout>

            </LinearLayout>

            <!--Nama-->
            <LinearLayout
                android:layout_marginTop="25dp"
                android:orientation="horizontal"
                android:paddingStart="25dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:ignore="RtlSymmetry">
                <de.hdodenhof.circleimageview.CircleImageView
                    android:layout_width="@dimen/_35sdp"
                    android:id="@+id/adminimgdetail"
                    android:layout_marginTop="10dp"
                    android:layout_height="@dimen/_35sdp"
                    app:civ_border_color="#000"
                    android:src="@drawable/user"
                    app:civ_border_width="2dp" />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <TextView
                        android:textSize="@dimen/_12ssp"
                        android:textStyle="bold"
                        android:fontFamily="@font/poppins_medium"
                        android:layout_gravity="center_vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingStart="20dp"
                        android:text="Nama Pelanggan"/>
                    <TextView
                        android:id="@+id/namadetialadmin"
                        android:textSize="@dimen/_14ssp"
                        android:textStyle="bold"
                        android:fontFamily="@font/poppins_medium"
                        android:layout_gravity="center_vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingStart="20dp"
                        android:text=" "/>
                </LinearLayout>
            </LinearLayout>
            <!--Telp-->
            <LinearLayout
                android:layout_marginTop="25dp"
                android:orientation="horizontal"
                android:paddingStart="25dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:ignore="RtlSymmetry">
                <de.hdodenhof.circleimageview.CircleImageView
                    android:layout_width="@dimen/_35sdp"
                    android:layout_marginTop="10dp"
                    android:layout_height="@dimen/_35sdp"
                    app:civ_border_color="#000"
                    app:civ_border_width="2dp"
                    android:background="@drawable/ic_phone"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <TextView
                        android:textSize="@dimen/_12ssp"
                        android:textStyle="bold"
                        android:fontFamily="@font/poppins_medium"
                        android:layout_gravity="center_vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingStart="20dp"
                        android:text="No Handphone"/>
                    <TextView
                        android:id="@+id/notelpadmin"
                        android:textSize="@dimen/_14ssp"
                        android:textStyle="bold"
                        android:fontFamily="@font/poppins_medium"
                        android:layout_gravity="center_vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingStart="20dp"
                        android:text=" "/>
                </LinearLayout>
            </LinearLayout>
            <!--Telp-->
            <LinearLayout
                android:layout_marginTop="25dp"
                android:orientation="horizontal"
                android:paddingStart="25dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:ignore="RtlSymmetry">
                <de.hdodenhof.circleimageview.CircleImageView
                    android:layout_width="@dimen/_35sdp"
                    android:layout_marginTop="10dp"
                    android:layout_height="@dimen/_35sdp"
                    app:civ_border_color="#000"
                    app:civ_border_width="2dp"
                    android:background="@drawable/ic_pin"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <TextView
                        android:textSize="@dimen/_12ssp"
                        android:textStyle="bold"
                        android:fontFamily="@font/poppins_medium"
                        android:layout_gravity="center_vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingStart="20dp"
                        android:text="Alamat"/>
                    <TextView
                        android:id="@+id/alamatdetailadmin"
                        android:textSize="@dimen/_14ssp"
                        android:textStyle="bold"
                        android:fontFamily="@font/poppins_medium"
                        android:layout_gravity="center_vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingStart="20dp"
                        android:text=" "/>
                </LinearLayout>
            </LinearLayout>
            <!--Telp-->
            <LinearLayout
                android:layout_marginTop="25dp"
                android:orientation="horizontal"
                android:paddingStart="25dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:ignore="RtlSymmetry">
                <de.hdodenhof.circleimageview.CircleImageView
                    android:layout_width="@dimen/_30sdp"
                    android:id="@+id/laundryprofile"
                    android:layout_marginTop="10dp"
                    android:layout_height="@dimen/_30sdp"
                    app:civ_border_color="#000"
                    app:civ_border_width="2dp"
                    android:background="@drawable/ic_calender"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <TextView
                        android:textSize="@dimen/_12ssp"
                        android:textStyle="bold"
                        android:fontFamily="@font/poppins_medium"
                        android:layout_gravity="center_vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingStart="20dp"
                        android:text="Tanggal"/>
                    <TextView
                        android:id="@+id/tanggaldetailadmin"
                        android:textSize="@dimen/_14ssp"
                        android:textStyle="bold"
                        android:fontFamily="@font/poppins_medium"
                        android:layout_gravity="center_vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingStart="20dp"
                        android:text=" "/>
                </LinearLayout>
            </LinearLayout>




            <!--Weight-->
            <LinearLayout
                android:layout_marginTop="25dp"
                android:orientation="horizontal"
                android:paddingStart="25dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:ignore="RtlSymmetry">
                <ImageView
                    android:layout_gravity="center"
                    android:layout_width="@dimen/_30sdp"
                    android:layout_height="@dimen/_30sdp"
                    android:src="@drawable/weight"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <TextView
                        android:textSize="@dimen/_12ssp"
                        android:textStyle="bold"
                        android:fontFamily="@font/poppins_medium"
                        android:layout_gravity="center_vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingStart="20dp"
                        android:text="Berat Pakaian"/>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/beratdetailadmin"
                            android:textStyle="bold"
                            android:textSize="@dimen/_13ssp"
                            android:fontFamily="@font/poppins_medium"
                            android:layout_gravity="center_vertical"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:paddingStart="20dp"
                            android:text="Belum ada Data"/>
                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>

            <!--Humidity-->
            <LinearLayout
                android:layout_marginTop="25dp"
                android:orientation="horizontal"
                android:paddingStart="25dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:ignore="RtlSymmetry">
                <ImageView
                    android:layout_gravity="center"
                    android:layout_width="@dimen/_30sdp"
                    android:layout_height="@dimen/_30sdp"
                    android:src="@drawable/humidity"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <TextView
                        android:textSize="@dimen/_12ssp"
                        android:textStyle="bold"
                        android:fontFamily="@font/poppins_medium"
                        android:layout_gravity="center_vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingStart="20dp"
                        android:text="Kelembapan"/>
                    <TextView
                        android:id="@+id/detailhumiadmin"
                        android:textStyle="bold"
                        android:textSize="@dimen/_13ssp"
                        android:fontFamily="@font/poppins_medium"
                        android:layout_gravity="center_vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingStart="20dp"
                        android:text="Belum ada Data"
                        tools:ignore="RtlSymmetry" />
                </LinearLayout>

            </LinearLayout>

            <!--Price-->
            <LinearLayout
                android:layout_marginTop="25dp"
                android:orientation="horizontal"
                android:paddingStart="25dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:ignore="RtlSymmetry">
                <ImageView
                    android:layout_gravity="center"
                    android:layout_width="@dimen/_30sdp"
                    android:layout_height="@dimen/_30sdp"
                    android:src="@drawable/ic_money"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <TextView
                        android:textSize="@dimen/_12ssp"
                        android:textStyle="bold"
                        android:fontFamily="@font/poppins_medium"
                        android:layout_gravity="center_vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingStart="20dp"
                        android:text="Harga"/>
                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <TextView
                            android:id="@+id/hargadetailadmin"
                            android:textStyle="bold"
                            android:textSize="@dimen/_13ssp"
                            android:fontFamily="@font/poppins_medium"
                            android:layout_gravity="center_vertical"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:paddingStart="20dp"
                            android:text="Belum ada Data"
                            tools:ignore="RtlSymmetry" />
                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>
            <Button
                android:layout_marginTop="@dimen/_15sdp"
                android:layout_gravity="center"
                android:id="@+id/updatedetailadmin"
                android:layout_width="@dimen/_220sdp"
                android:layout_height="40dp"
                android:background="@drawable/round_blue"
                android:fontFamily="@font/acme"
                android:foreground="?android:attr/selectableItemBackground"
                android:text="Update"
                android:textColor="#FFF"
                android:textSize="@dimen/_13ssp"
                android:textStyle="bold"
                />
        </LinearLayout>
    </LinearLayout>


</ScrollView>